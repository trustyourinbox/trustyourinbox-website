    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
