{
  "name": "trustyourinbox-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <26.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
