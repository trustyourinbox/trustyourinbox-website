    "@sendgrid/mail": "^8.1.6",
    "@tailwindcss/postcss": "^4.1.17",
    "@vercel/speed-insights": "^1.2.0",
    "adm-zip": "^0.5.16",
    "asn1.js": "^5.4.1",
    "cidr-tools": "^11.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
